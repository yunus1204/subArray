
import java.util.*;


public class Main
{
	public static void main(String[] args) {
		int arr[]={4,2,-3,1,6};
		int n=arr.length;
		for(int i=0;i<n;i++){
		    for(int j=i;j<n;j++){
		        int p=0;
		        int sub[]=new int[j-i+1];
		        for(int k=i;k<=j;k++){
		            sub[p]=arr[k];
		            p++;
		        }
		        System.out.println(Arrays.toString(sub));
		    }
		}
	}
}
